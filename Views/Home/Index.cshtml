@model WebVuiVN.Models.ViewModels.HomeViewModel
@{
    ViewData["Title"] = Model.Title;
    ViewData["NameUser"] = Model.User.FullName;
}
<script crossorigin=crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react/16.13.0/umd/react.development.js">
</script>
<script crossorigin=crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.13.0/umd/react-dom.development.js">
</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/remarkable/1.7.1/remarkable.min.js">
</script>
<script src="@Url.Content("~/appclient/src/router/index.tsx")">
</script>
<main class="main-content mt-4">
    <div class="container">
        <div class="row">
            <!--USER PROFILE COULMN-->
            <section class="col col-lg-3 user-profile text-center">
                <div class="card">
                    <div class="card-head">
                        <div class="user-profile__cover-photo">
                            <img src="~/Socialnetwork/img/user-cover-photo.jpg" alt="" class="img-fluid w-100" />
                        </div>
                        <div class="user-profile__pp">
                            <img src="~/Socialnetwork/img/img-pp.png" alt="" class="ml-auto mr-auto d-block img-fluid" />
                        </div>
                    </div>
                    <div class="card-body">
                        <h2 class="card-title user-profile__name h5">@Model.User.FullName</h2>
                        <p class="card-text user-profile__profession">Creative UI/UX Designer</p>
                    </div>
                    <div class="card-footer user-profile__stats d-flex justify-content-between bg-white border-0 pt-4">
                        <div class="user-profile__stat">
                            <div class="ups__title">Posts</div>
                            <div class="ups__count text-primary">200</div>
                        </div>
                        <div class="user-profile__stat">
                            <div class="ups__title">Followers</div>
                            <div class="ups__count text-primary">999</div>
                        </div>
                        <div class="user-profile__stat">
                            <div class="ups__title">Following</div>
                            <div class="ups__count text-primary">520</div>
                        </div>
                    </div>
                </div>
            </section>
            <!--=================== news feed column ===================-->
            <section class="col-12 col-lg-9 col-xl-6 user-feed">
                <!--add post-->
                <div id="add-new-post">
                    <div class="user_info">
                        <img src="~/Socialnetwork/img/img-pp.png" alt="" class="ml-auto mr-auto d-block img-fluid" />
                    </div>
